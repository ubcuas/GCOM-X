{
  "name": "gcomx-frontend",
  "version": "0.0.1",
  "private": true,
  "description": "gcomx frontend assets.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "start": "babel-node server.js",
    "build": "NODE_ENV=production webpack -p --progress --colors --config webpack.prod.config.js --bail",
    "lint": "eslint assets",
    "coverage": "jest --coverage"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/jest-css-modules"
    },
    "collectCoverageFrom": [
      "assets/js/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "assets/js/store.js",
      "assets/js/index.js",
      "assets/js/jquery-index.js",
      "assets/js/constants/*",
      "assets/js/pages/*",
      "assets/js/tests/*"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 10
      }
    },
    "modulePaths": [
      "assets",
      "assets/js",
      "assets/js/app"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/tests/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/tests/test-env.js",
      "<rootDir>/tests/setupEnzyme.js"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "autoprefixer": "^7.2.6",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.2.1",
    "bootstrap-loader": "^2.2.0",
    "classnames": "^2.2.6",
    "css-loader": "^0.28.11",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "imports-loader": "^0.7.1",
    "jquery": "^3.3.1",
    "js-file-download": "^0.4.7",
    "leaflet": "^1.4.0",
    "leaflet-defaulticon-compatibility": "^0.1.1",
    "leaflet-draw": "^1.0.4",
    "leaflet.offline": "^1.0.4",
    "localforage": "^1.7.3",
    "lodash": "^4.17.11",
    "marked": "^0.3.19",
    "node-sass": "^4.11.0",
    "popper.js": "^1.14.6",
    "postcss": "^6.0.23",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.2",
    "react": "^15.6.2",
    "react-canvas-knob": "github:0x6a77/react-canvas-knob#b142e09a843652730a7ecbf2c31eeb5adb056b49",
    "react-dom": "^15.6.2",
    "react-dropdown": "^1.6.3",
    "react-interval-timer": "^1.0.5",
    "react-key-handler": "^1.2.0-beta.3",
    "react-keyboard-event-handler": "^1.4.1",
    "react-leaflet": "^1.9.1",
    "react-leaflet-draw": "^0.18.0",
    "react-modal": "^3.8.1",
    "react-redux": "^5.1.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-select": "^2.2.0",
    "react-switch": "^4.1.0",
    "react-table": "^6.8.6",
    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.6.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "resolve-url-loader": "^2.3.1",
    "sass-loader": "^6.0.7",
    "script-loader": "^0.7.2",
    "sockette": "^2.0.5",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.12.0",
    "webpack-bundle-tracker": "^0.3.0",
    "webpack-dev-server": "^2.11.3",
    "webpack-spritesmith": "^0.5.4",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "ajv": "^6.6.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-15": "^1.2.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.12.3",
    "jest": "^24.7.1",
    "jest-css-modules": "^1.1.0",
    "mock-socket": "^8.0.5",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.6.2"
  }
}
